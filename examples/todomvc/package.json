{
  "name": "todomvc",
  "version": "0.0.1",
  "license": "mit",
  "private": true,
  "scripts": {
    "postinstall": "bower install",
    "build": "pulp -w browserify -m Main -t public/index.js",
    "build-o": "pulp build -O -m Main -t .build/index.js && browserify -g [ envify --NODE_ENV production ] -g uglifyify -e .build/index.js -o public/index.js",
    "start": "http-server public",
    "dev": "npm run start & npm run build"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "envify": "^4.0.0",
    "enzyme": "^2.4.1",
    "react-addons-test-utils": "^15.3.0",
    "react-scripts": "^0.6.0",
    "uglifyify": "^3.0.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "envify": "^4.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-props": "0.0.3",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "todomvc-app-css": "^2.1.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
